 <script>
    const agentDropdown = document.getElementById('agent-field');
    const countryDropdown = document.getElementById('country-dropdown');
    const addCountryButton = document.getElementById('add-country-field');
    const countryFieldsContainer = document.getElementById('country-fields-container');
    const serviceChargeField = document.getElementById('service-charge-field');

    // Function to check if both country and passport number fields are filled
    function areFieldsFilled() {
        const countryInputs = document.querySelectorAll('input[name="passportnumber[]"]');
        const passengerNameInputs = document.querySelectorAll('input[name="passengername[]"]');
        
        // Set desired width for country inputs
        const desiredWidth = '500px';
        countryInputs.forEach(input => {
            input.style.width = desiredWidth;
        });

        // Check if any of the fields are filled
        return Array.from(countryInputs).some(input => input.value.trim() !== '')
            || Array.from(passengerNameInputs).some(input => input.value.trim() !== '');
    }

    // Function to toggle the visibility of the "Add Field" button
    function toggleAddButtonVisibility() {
        const addButton = document.getElementById('add-country-field');
        addButton.style.display = areFieldsFilled() ? 'block' : 'none';
    }

    // Add input event listeners to country and passport number fields
    const countryInputs = document.querySelectorAll('input[name="passportnumber[]"]');
    const passengerNameInputs = document.querySelectorAll('input[name="passengername[]"]');
    
    countryInputs.forEach(input => {
        input.addEventListener('input', toggleAddButtonVisibility);
    });

    passengerNameInputs.forEach(input => {
        input.addEventListener('input', toggleAddButtonVisibility);
    });

    // Initially hide the "Add Field" button
    toggleAddButtonVisibility();



    // Store selected countries for each agent
    const selectedCountries = {};

 function populateCountryDropdown(selectedAgentId) {
            const countryDropdown = document.getElementById('country-dropdown');
            countryDropdown.innerHTML = ''; // Clear the dropdown

            // Replace 'YOUR_API_ENDPOINT' with the actual API endpoint
            fetch(`/talab/createinvoice/:id?agentId=${selectedAgentId}`)
                .then((response) => {
                    if (!response.ok) {
                        throw new Error('API request failed');
                    }
                    return response.json();
                })
                .then((servicechargeData) => {
                    // Check if the response is an array and not empty
                    if (Array.isArray(servicechargeData) && servicechargeData.length > 0) {
                        // Create an initial "Select a country" option
                        const initialOption = document.createElement('option');
                        initialOption.value = '';
                        initialOption.textContent = 'Select a country';
                        countryDropdown.appendChild(initialOption);

                        // Populate the dropdown with countries from the response
                        servicechargeData.forEach((item) => {
                            const option = document.createElement('option');
                            option.value = item.country;
                            option.textContent = item.country;
                            countryDropdown.appendChild(option);
                            
                        });
                        console.log(servicechargeData,"DATA>>>>>>>>>>>>>>>>>>>>>>>")
                        

                    } else {
                        // If no countries are found, display a message
                        const noCountriesOption = document.createElement('option');
                        noCountriesOption.textContent = 'No Countries Found';
                        countryDropdown.appendChild(noCountriesOption);
                    }
                })
                
                .catch((error) => {
                    console.error(error);
                    // Handle API errors here
                });
        }

        // Call the function with the selected agent ID (replace with the actual value)

    agentDropdown.addEventListener('change', (event) => {
        const selectedAgentId = event.target.value;
        console.log('Selected Agent ID:', selectedAgentId);
        populateCountryDropdown(selectedAgentId);
    });

    addCountryButton.addEventListener('click', () => {
        const newField = document.createElement('div');
        newField.classList.add('country-price-field');

        // Clone the existing dropdown and its options
        const clonedCountryDropdown = countryDropdown.cloneNode(true);

        // Create a new div to hold passport number and passenger name inputs and delete button
        const inputFieldsDiv = document.createElement('div');
        inputFieldsDiv.classList.add('add_input_fields'); // Add your desired class name

        // Create the delete button but initially hide it
        const deleteButton = document.createElement('button');
        deleteButton.type = 'button';
        deleteButton.classList.add('btn', 'btn-outline-danger', 'remove-field');
        deleteButton.innerHTML = '<i class="fas fa-trash trash-icon"></i>';
        deleteButton.style.display = 'none'; // Initially hide the delete button

        // Create passport number input
        const passportNumberInput = document.createElement('input');
        passportNumberInput.type = 'text';
        passportNumberInput.name = 'passportnumber[]';
        passportNumberInput.placeholder = 'Enter passport number';
        passportNumberInput.classList.add('form-control');

        // Create passenger name input
        const passengerNameInput = document.createElement('input');
        passengerNameInput.type = 'text';
        passengerNameInput.name = 'passengername[]';
        passengerNameInput.placeholder = 'Enter passenger name';
        passengerNameInput.classList.add('form-control');

        // Add the delete button, passport number input, and passenger name input to the inputFieldsDiv
        inputFieldsDiv.appendChild(deleteButton);
        inputFieldsDiv.appendChild(passportNumberInput);
        inputFieldsDiv.appendChild(passengerNameInput);

        // Add the clonedCountryDropdown and inputFieldsDiv to the newField div
        newField.appendChild(clonedCountryDropdown);
        newField.appendChild(inputFieldsDiv);

        countryFieldsContainer.appendChild(newField);

        // Clear input values to reset the fields
        clonedCountryDropdown.value = '';
        passportNumberInput.value = '';
        passengerNameInput.value = '';

        // Show the delete button for the new field
        deleteButton.style.display = 'inline-block';

        // Add an event listener to the delete button
        deleteButton.addEventListener('click', () => {
            // Find the parent field using DOM traversal
            const fieldToDelete = deleteButton.closest('.country-price-field');

            // Remove the field
            if (fieldToDelete) {
                fieldToDelete.remove();
            }

            // Check and toggle the "Add Field" button visibility after deleting
            toggleAddButtonVisibility();
        });

        // Hide the "Add Field" button after adding a new field
        toggleAddButtonVisibility();
    });

</script>
